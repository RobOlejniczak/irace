<!DOCTYPE html>
<html lang="en-US">
 <head>
  <title>{{ league["leaguename"] }} at {{ race.race["track_name"] }}</title>
  <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">
  <script type="text/javascript" language="javascript" src="https://code.jquery.com/jquery-3.3.1.js"></script>
  <script type="text/javascript" language="javascript" src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
  <script type="text/javascript" class="init">
   $(document).ready(function() {
    $("#results").DataTable({
      "order": [[ 2, "asc" ]],
      "lengthMenu": [[25, -1], [25, "All"]]
    });
   });
  </script>
 </head>
 <body>
  <p><a href='/{{ league["leagueid"] }}.html'>{{ league["leaguename"] }}</a> <a href='/{{ league["leagueid"] }}/seasons/{{ season["league_season_id"] }}.html'>{{ season["league_season_name"] }}</a> at {{ race.race["track_name"] }}</p>
  <table id="results">
   <thead>
    <tr>
     <th>Name</th>
     <th>Interval</th>
     <th>Finish</th>
     <th>Start</th>
     <th>Result</th>
     <th>Laps</th>
     <th>Fast Lap</th>
     <th>Fastest Lap</th>
     <th>Incidents</th>
     <th>Points</th>
    </tr>
   </thead>
   <tbody>
    {%- for result in race.results %}
    <tr>
     <td><a href='/{{ league["leagueid"] }}/seasons/{{ season["league_season_id"] }}/{{ race.race["subsessionid"] }}/{{ result["custid"] }}.html'>{{ result["displayname"]}}</a></td>
     <td>{% if result["interval"] > 0 or result["finishpos"] == 0 %}{{ time_string_raw(result["interval"]) }}{% else %}{{ result["lapscomplete"] - race.race["eventlapscomplete"] }}L{% endif %}</td>
     <td>{{ result["finishpos"] + 1 }}</td>
     <td>{{ result["startpos"] + 1 }}</td>
     <td>{{ result["reasonout"] }}</td>
     <td><a href='/{{ league["leagueid"] }}/seasons/{{ season["league_season_id"] }}/{{ race.race["subsessionid"] }}/{{ result["custid"] }}.html'>{{ result["lapscomplete"] }}</a></td>
     <td>{% if result["bestlapnum"] > 0 %}{{ result["bestlapnum"] }}{% else %}-{% endif %}</td>
     <td>{{ time_string_raw(result["bestlaptime"]) }}</td>
     <td>{{ result["incidents"] }}</td>
     <td>{{ result["league_points"] }}</td>
    </tr>
    {%- endfor %}
   </tbody>
  </table>
 </body>
</html>
